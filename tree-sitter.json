{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dhall",
      "camelcase": "Dhall",
      "title": "Dhall",
      "scope": "source.dhall",
      "file-types": ["dhall"],
      "injection-regex": "^dhall$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "class-name": "TreeSitterDhall"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Dhall grammar for the tree-sitter parsing library",
    "authors": [
      {
        "name": "jae beller",
        "email": "foss@jae.zone"
      }
    ],
    "links": {
      "repository": "https://github.com/jbellerb/tree-sitter-dhall"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
